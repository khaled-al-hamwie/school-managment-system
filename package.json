  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:repl": "npm run start -- --entryFile repl",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:unseed": "npx sequelize-cli db:seed:undo:all",
    "db:reset": "npm run db:unseed && npm run db:seed",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch --config ./jest.json --detectOpenHandles --forceExit",
    "test:cov": "jest --coverage --config ./jest.json --detectOpenHandles --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
